Aim= Five numbers summary in exploratory data analysis


code

# Load necessary libraries
library(dplyr)

# Sample data
data <- data.frame(values = c(10, 12, 15, 8, 20, 18, 11, 14, 16, 9))

# Calculate five-number summary
five_number_summary <- data %>% 
  summarise(
    minimum = min(values),
    Q1 = quantile(values, 0.25),
    median = median(values), 
    Q3 = quantile(values, 0.75), 
    maximum = max(values)
  )

# Print results
print(five_number_summary)
